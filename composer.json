{
  "name": "tentwofour/symfony-twig-bridge",
  "type": "library",
  "description": "Symfony implementation of the tentwofour/twig package",
  "keywords": [
    "ten24",
    "tentwofour",
    "twig",
    "symfony"
  ],
  "homepage": "https://github.com/tentwofour/SymfonyTwigBridge",
  "license": "MIT",
  "authors": [
    {
      "name": "Blair D. Patterson",
      "homepage": "http://tentwentyfour.ca",
      "email": "blair@tentwentyfour.ca"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "tentwofour/twig": "~1.0",
    "symfony/dependency-injection": "~2.3|~3.0",
    "symfony/config": "~2.3|~3.0",
    "symfony/http-kernel": "~2.3|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-0": {
      "Ten24\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
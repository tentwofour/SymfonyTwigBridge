{
  "name": "tentwofour/symfony-twig-bridge",
  "type": "library",
  "description": "Symfony implementation of the tentwofour/twig package",
  "keywords": [
    "ten24",
    "tentwofour",
    "twig",
    "symfony"
  ],
  "homepage": "https://github.com/tentwofour/SymfonyTwigBridge",
  "license": "MIT",
  "authors": [
    {
      "name": "Blair D. Patterson",
      "homepage": "http://tentwentyfour.ca",
      "email": "blair@tentwentyfour.ca"
    }
  ],
  "require": {
    "php": "^7.0",
    "tentwofour/twig": "^1.1",
    "symfony/dependency-injection": "^3.0|^4.0|^5.0",
    "symfony/config": "^3.0|^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "symfony/http-kernel": "^3.0|^4.0",
    "symfony/yaml": "^3.0|^4.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Ten24\\Bundle\\TwigBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ten24\\Tests\\Bundle\\TwigBundle\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}